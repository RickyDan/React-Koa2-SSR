{
  "presets": ["es2015", ["env", { "modules": false }], "react", "stage-2"],
  "plugins": [
    "add-module-exports",
    "transform-runtime",
    "transform-decorators-legacy",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    "lodash"
  ],
  "comments": false
}
